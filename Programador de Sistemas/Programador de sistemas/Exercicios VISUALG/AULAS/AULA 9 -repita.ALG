algoritmo "Aula 9 -treinoo "

var
   soma, vald:real


inicio
   escreval("=========================")

   soma<-0
   repita
      escreva("Digite um valor: ")
      leia(vald)

      soma<-soma+vald
      escreval("Total Parcial: ", soma)

   ate vald=0

   escreval("=========================")
   escreval("Resultado: ",soma)





fimalgoritmo