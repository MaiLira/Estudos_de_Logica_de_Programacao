algoritmo "Enquanto - Exercicios"
var
cont, num,salto:inteiro
inicio
   escreva("Digite um número que deseja contar: ")
   leia(num)
   escreva("Digite o salto desejado: ")
   leia(salto)
   
cont<-0
enquanto (cont<=num) faca
escreval(cont)
cont<-cont+salto

fimenquanto

fimalgoritmo